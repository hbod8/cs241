        [34m/* Create temp arrays */[39m[K[11;80H        [32mint[39m L[] = [33mnew[39m [32mint[39m[n1];[K[12;80H        [32mint[39m R[] = [33mnew[39m [32mint[39m[n2];[K[13;80H[K
        [34m/*Copy data to temp arrays*/[39m[K[15;80H        [33mfor[39m ([32mint[39m i=[31m0[39m; i<n1; i++) {[K[16;80H            L[i] = arr[l+i];[K[17;80H        }[K[18;80H        [33mfor[39m ([32mint[39m j=[31m0[39m; j<n2; j++) {[K[19;80H            R[j] = arr[m+[31m1[39m+j];[K[20;80H        }[K[21;80H[K
