    [34m/* A utility function to print array of size n */[39m[K[67;80H    [32mstatic[39m [32mvoid[39m printArray([32mint[39m arr[]) {[K[68;80H                                                                                                           10,9          25%[K[1;88H[?12l[?25h[?25l[8D        [32mint[39m n1 = m - l;[K[2;80H        [32mint[39m n2 = r - m;[K[3;80H[K
