        [34m// Initial index of merged subarry array[39m[K[29;95H        [32mint[39m k = l-[31m1[39m;[K[30;95H        [33mwhile[39m (i < n1 && j < n2) {[K[31;95H            [33mif[39m (L[i] <= R[j]) {[K[32;95H                arr[k] = L[i];[K[33;95H                i++;[K[34;95H            }[K[35;95H            [33melse[39m {[K[36;95H                arr[k] = R[j];[K[37;95H                j++;[K[38;95H            }[K[39;95H            k++;[K[40;95H        }[K[41;95H[K
