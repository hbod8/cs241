                                                                                                       [1;105H{[K[2;105H    [34m// Merges two subarrays of arr[].[39m[K[3;105H    [34m// First subarray is arr[l..m][39m[K[4;105H    [34m// Second subarray is arr[m+1..r][39m[K[5;105H    [32mprivate[39m [32mstatic[39m [32mvoid[39m merge([32mint[39m arr[], [32mint[39m l, [32mint[39m m, [32mint[39m r) {[K[6;105H        [34m// Find sizes of two subarrays to be merged[39m[K[7;105H        [32mint[39m n1 = m - l;[K[8;105H        [32mint[39m n2 = r - m;[K[9;105H[K
        [34m/* Create temp arrays */[39m[K[11;105H        [32mint[39m L[] = [33mnew[39m [32mint[39m[n1];[K[12;105H        [32mint[39m R[] = [33mnew[39m [32mint[39m[n2];[K[13;105H[K
        [34m/*Copy data to temp arrays*/[39m[K[15;105H        [33mfor[39m ([32mint[39m i=[31m0[39m; i<n1; i++) {[K[16;105H            L[i] = arr[l+i];[K[17;105H        }[K[18;105H        [33mfor[39m ([32mint[39m j=[31m0[39m; j<n2; j++) {[K[19;105H            R[j] = arr[m+[31m1[39m+j];[K[20;105H        }[K[21;105H[K
[K
        [34m/* Merge the temp arrays */[39m[K[24;105H[K
        [34m// Initial indexes of first and second subarrays[39m[K[26;105H        [32mint[39m i = [31m0[39m, j = [31m0[39m;[K[27;105H[K
        [34m// Initial index of merged subarry array[39m[K[29;105H        [32mint[39m k = l-[31m1[39m;[K[30;105H        [33mwhile[39m (i < n1 && j < n2) {[K[31;105H            [33mif[39m (L[i] <= R[j]) {[K[32;105H                arr[k] = L[i];[K[33;105H                i++;[K[34;105H            }[K[35;105H            [33melse[39m {[K[36;105H                arr[k] = R[j];[K[37;105H                j++;[K[38;105H            }[K[39;105H            k++;[K[40;105H        }[K[41;105H[K
        [34m/* Copy remaining elements of L[] if any */[39m[K[43;105H        [33mwhile[39m (i < n1) {[K[44;105H            arr[k] = L[i];[K[45;105H            i++;[K[46;105H            k++;[K[47;105H        }[K[48;105H[K
        [34m/* Copy remaining elements of R[] if any */[39m[K[50;105H        [33mwhile[39m (j < n2) {[K[51;105H            arr[k] = R[j];[K[52;105H            j++;[K[53;105H            k++;[K[54;105H        }[K[55;105H    }[K[56;105H[K
    [34m// Main function that sorts arr[l..r] using[39m[K[58;105H    [34m// merge()[39m[K[59;105H    [32mprivate[39m [32mstatic[39m [32mvoid[39m sort([32mint[39m arr[], [32mint[39m l, [32mint[39m r) {[K[60;105H        [33mif[39m (l < r) {[K[61;105H            [34m// Find the middle point[39m[K[62;105H            [32mint[39m m = (l+r)/[31m2[39m;[K[63;105H[K
            [34m// Sort first and second halves[39m[K[65;105H            sort(arr, l, m);[K[66;105H            sort(arr , m+[31m1[39m, r);[K[67;105H[K
                                                                                   13,1           5%[68;204H
