        [34m/*Copy data to temp arrays*/[39m[K[15;95H        [33mfor[39m ([32mint[39m i=[31m0[39m; i<n1; i++) {[K[16;95H            L[i] = arr[l+i];[K[17;95H        }[K[18;95H        [33mfor[39m ([32mint[39m j=[31m0[39m; j<n2; j++) {[K[19;95H            R[j] = arr[m+[31m1[39m+j];[K[20;95H        }[K[21;95H[K
