        [34m/* Merge the temp arrays */[39m[K[24;91H[K
        [34m// Initial indexes of first and second subarrays[39m[K[26;91H        [32mint[39m i = [31m0[39m, j = [31m0[39m;[K[27;91H[K
        [34m// Initial index of merged subarry array[39m[K[29;91H        [32mint[39m k = l-[31m1[39m;[K[30;91H        [33mwhile[39m (i < n1 && j < n2) {[K[31;91H            [33mif[39m (L[i] <= R[j]) {[K[32;91H                arr[k] = L[i];[K[33;91H                i++;[K[34;91H            }[K[35;91H            [33melse[39m {[K[36;91H                arr[k] = R[j];[K[37;91H                j++;[K[38;91H            }[K[39;91H            k++;[K[40;91H        }[K[41;91H[K
