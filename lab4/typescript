                                                                                         [1;91H{[K[2;91H    [34m// Merges two subarrays of arr[].[39m[K[3;91H    [34m// First subarray is arr[l..m][39m[K[4;91H    [34m// Second subarray is arr[m+1..r][39m[K[5;91H    [32mprivate[39m [32mstatic[39m [32mvoid[39m merge([32mint[39m arr[], [32mint[39m l, [32mint[39m m, [32mint[39m r) {[K[6;91H        [34m// Find sizes of two subarrays to be merged[39m[K[7;91H        [32mint[39m n1 = m - l;[K[8;91H        [32mint[39m n2 = r - m;[K[9;91H[K
        [34m/* Create temp arrays */[39m[K[11;91H        [32mint[39m L[] = [33mnew[39m [32mint[39m[n1];[K[12;91H        [32mint[39m R[] = [33mnew[39m [32mint[39m[n2];[K[13;91H[K
        [34m/*Copy data to temp arrays*/[39m[K[15;91H        [33mfor[39m ([32mint[39m i=[31m0[39m; i<n1; i++) {[K[16;91H            L[i] = arr[l+i];[K[17;91H        }[K[18;91H        [33mfor[39m ([32mint[39m j=[31m0[39m; j<n2; j++) {[K[19;91H            R[j] = arr[m+[31m1[39m+j];[K[20;91H        }[K[21;91H[K
