        [34m/*Copy data to temp arrays*/[39m[K[16;104H        [33mfor[39m ([32mint[39m i=[31m0[39m; i<n1; i++) {[K[17;104H            L[i] = arr[l+i];[K[18;104H        }[K[19;104H        [33mfor[39m ([32mint[39m j=[31m0[39m; j<n2; j++) {[K[20;104H            R[j] = arr[m+[31m1[39m+j];[K[21;104H        }[K[22;104H[K
[K
        [34m/* Merge the temp arrays */[39m[K[25;104H[K
        [34m// Initial indexes of first and second subarrays[39m[K[27;104H        [32mint[39m i = [31m0[39m, j = [31m0[39m;[K[28;104H[K
        [34m// Initial index of merged subarry array[39m[K[30;104H        [32mint[39m k = l-[31m1[39m;[K[31;104H        [33mwhile[39m (i < n1 && j < n2) {[K[32;104H            [33mif[39m (L[i] <= R[j]) {[K[33;104H                arr[k] = L[i];[K[34;104H                i++;[K[35;104H            }[K[36;104H            [33melse[39m {[K[37;104H                arr[k] = R[j];[K[38;104H                j++;[K[39;104H            }[K[40;104H            k++;[K[41;104H        }[K[42;104H[K
        [34m/* Copy remaining elements of L[] if any */[39m[K[44;104H        [33mwhile[39m (i < n1) {[K[45;104H            arr[k] = L[i];[K[46;104H            i++;[K[47;104H            k++;[K[48;104H        }[K[49;104H[K
        [34m/* Copy remaining elements of R[] if any */[39m[K[51;104H        [33mwhile[39m (j < n2) {[K[52;104H            arr[k] = R[j];[K[53;104H            j++;[K[54;104H            k++;[K[55;104H        }[K[56;104H    }[K[57;104H[K
